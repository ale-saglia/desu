    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-App<desktop-client>",
            "request": "launch",
            "mainClass": "App",
            "projectName": "desktop-client",
            //"vmArgs": "--module-path /path/to/JavaFX/lib --add-modules=javafx.controls"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-Launcher<desktop-client>",
            "request": "launch",
            "mainClass": "Launcher",
            "projectName": "desktop-client"
        }
    ]
}